using System;

namespace Triangle
{
    class Triangle
    {
        static void Main()
        {
            Console.WriteLine("    \u00A9    ");
            Console.WriteLine("   \u00A9 \u00A9   ");
            Console.WriteLine("  \u00A9   \u00A9  ");
            Console.WriteLine(" \u00A9     \u00A9 ");
            Console.WriteLine("\u00A9\u00A9\u00A9\u00A9\u00A9\u00A9\u00A9\u00A9\u00A9");
        }
    }
}
